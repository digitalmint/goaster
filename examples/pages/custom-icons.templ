package pages

import (
	"github.com/digitalmint/goaster"
	"github.com/digitalmint/goaster/examples/layouts"
	"github.com/digitalmint/goaster/examples/layouts/partials"
)

templ CustomIconsPage(result bool, toaster *goaster.Toaster) {
	@layouts.Page() {
		@partials.Head("goaster - single toast")
		@partials.Body() {
			<!-- display the messages -->
			if result {
				@toaster.Success("Success Toast")
			} else {
				@toaster.Error("Error Toast")
			}
		}
	}
}
